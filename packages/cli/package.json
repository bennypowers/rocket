{
  "name": "@dakmor/cli",
  "version": "0.0.1",
  "publishConfig": {
    "access": "public"
  },
  "description": "Rocket stuff",
  "license": "MIT",
  "author": "Thomas Allmer",
  "main": "index.js",
  "bin": {
    "rocket-start": "src/start/cli.js",
    "rocket-build": "src/build/cli.js"
  },
  "scripts": {
    "start": "npm run rocket:start",
    "rocket:start": "node src/start/cli.js -c demo/docs --root-dir ../../ --open packages/cli/demo/docs/README.md",
    "rocket:build": "node src/build/cli.js -c demo/docs",
    "test": "mocha test/**/*.test.js test/*.test.js",
    "test:watch": "mocha test/**/*.test.js test/*.test.js --watch"
  },
  "files": [
    "*.js",
    "src"
  ],
  "keywords": [
    "storybook",
    "demo",
    "demo-states",
    "testing"
  ],
  "dependencies": {
    "@11ty/eleventy": "^0.11.0",
    "command-line-args": "^5.1.1",
    "command-line-usage": "^6.1.0",
    "es-dev-server": "^1.54.0",
    "@dakmor/eleventy-plugin-mdjs": "^0.0.1"
  },
  "devDependencies": {
    "patch-package": "^6.2.2"
  }
}
