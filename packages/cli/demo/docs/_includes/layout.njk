<!doctype html>
<html lang="en">

  <head>
    <link rel="stylesheet" href="/_assets/styles.css">
  </head>

  <body>
    <main>
      {{ content.html | safe }}
    </main>

    <header>
      Logo
    </header>

    <nav>
      <rocket-navigation>
        {{ collections.all | eleventyNavigation | eleventyNavigationToHtml({
            activeListItemClass: "current",
            activeKey: eleventyNavigation.key
        }) | safe }}
      </rocket-navigation>
    </nav>

    {% if content.jsCode %}
      <script type="module">
        {{ content.jsCode | safe }}
      </script>
    {% endif %}

    <script type="module">
      import '@dakmor/navigation/rocket-navigation.js';
    </script>
  </body>

</html>
